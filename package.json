{
  "name": "react-redux-typescript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.26",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.6",
    "@types/react-router-dom": "^4.0.7",
    "@types/redux-logger": "^3.0.3",
    "@types/redux-saga": "^0.10.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.6.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
